{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cwp/form-button",
	"title": "Button",
	"category": "gutenberg-forms",
	"supports": {
		"align": ["left", "center", "right"]
	},
	"ancestor": [ "cwp/block-gutenberg-forms" ],
	"keywords": [
		"button",
		"submit",
		"input",
		"form"
	],
	"version": "1.0.0",
	"textdomain": "forms-gutenberg",
	"attributes": {
		"label": {
			"type": "string",
			"default": "Submit"
		},
		"parentId": {
			"type": "string",
			"default": ""
		},
		"action": {
			"default": "submit",
			"type": "string"
		},
		"styling": {
			"type": "object",
			"default": {
				"backgroundColor": "rgb(238, 238, 238)",
				"color": "rgb(49, 49, 49)",
				"padding": 25,
				"borderRadius": 0
			}
		}
	}
}
