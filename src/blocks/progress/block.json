{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 1,
	"name": "cwp/progress",
	"title": "Progress Bar",
	"category": "gutenberg-forms",
	"ancestor": [ "cwp/block-gutenberg-forms" ],
	"keywords": [
		"progress",
		"bar",
		"form"
	],
	"version": "1.0.0",
	"textdomain": "forms-gutenberg",
	"attributes": {
		"progressColor": {
			"type": "string",
			"default": "rgb(238, 238, 238)"
		},
		"progressFillColor": {
			"type": "string",
			"default": "rgb(6, 147, 227)"
		},
		"thickness": {
			"type": "number",
			"default": 20
		},
		"cornerRadius": {
			"type": "number",
			"default": 0
		},
		"showPercentage": {
			"type": "boolean",
			"default": true
		},
		"textColor": {
			"type": "string",
			"default": "rgb(238, 238, 238)"
		}
	}
}
